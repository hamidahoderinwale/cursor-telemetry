{
  "name": "@cursor-telemetry/native",
  "version": "1.0.0",
  "description": "Native Rust module for high-performance operations",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "cursor-telemetry-native",
    "triples": {
      "defaults": true,
      "additional": [
        "aarch64-apple-darwin",
