{
  "name": "pkl-memory-extension",
  "displayName": "PKL Memory Integration",
  "description": "Integrates PKL dashboard memories with Cursor",
  "version": "1.0.0",
  "publisher": "pkl-dashboard",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:pkl-memory.showMemories"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "pkl-memory.showMemories",
        "title": "Show PKL Memories",
        "category": "PKL"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "pkl-memories",
          "name": "PKL Memories",
          "when": "true"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "pkl-memory.showMemories",
          "when": "view == pkl-memories",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  }
}
