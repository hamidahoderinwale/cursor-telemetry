{
  "name": "@cursor-telemetry/sdk",
  "version": "1.1.0",
  "description": "Official JavaScript/TypeScript SDK for Cursor Telemetry API with Privacy Controls and Activity Logger Integration",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc && npm run build:types",
    "build:types": "tsc --emitDeclarationOnly",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run build",
    "docs": "typedoc src/index.ts --out docs",
    "example": "ts-node examples/basic-usage.ts"
  },
  "keywords": [
    "cursor",
    "telemetry",
    "api",
    "sdk",
    "dashboard",
    "analytics",
    "jupyter",
    "notebook",
    "data-science",
    "typescript",
    "privacy",
    "activity-logger",
    "real-time",
    "monitoring"
  ],
  "author": "Cursor Telemetry Team <team@cursor-telemetry.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cursor-telemetry/sdk.git",
    "directory": "javascript"
  },
  "bugs": {
    "url": "https://github.com/cursor-telemetry/sdk/issues"
  },
  "homepage": "https://github.com/cursor-telemetry/sdk#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "socket.io-client": "^4.7.2",
    "ws": "^8.14.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.0",
    "@types/uuid": "^9.0.0",
    "@types/jest": "^29.0.0",
    "typescript": "^5.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "prettier": "^3.0.0",
    "typedoc": "^0.25.0"
  },
  "peerDependencies": {
    "typescript": ">=4.5.0"
  }
}
